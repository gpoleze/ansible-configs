---
apiVersion: v1
kind: ConfigMap
metadata:
  name: torrent-config
  namespace: "{{ namespace }}"
data:
  PUID: "{{ nfs_puid }}"
  PGID: "{{ nfs_pgid }}"
  UMASK: "22"

---
apiVersion: v1
kind: ConfigMap
metadata:
  name: vpn-config
  namespace: "{{ namespace }}"
data:
  CONNECT: --group p2p
  TECHNOLOGY: NordLynx